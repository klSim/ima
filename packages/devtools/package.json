{
  "name": "@ima/devtools",
  "version": "0.3.2",
  "description": "IMA.js debugging panel in the Chrome Developer Tools window.",
  "keywords": [
    "IMA",
    "IMA.js",
    "Chrome",
    "Devtools",
    "Javascript"
  ],
  "bugs": {
    "url": "https://github.com/seznam/ima/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seznam/ima.git",
    "directory": "packages/devtools"
  },
  "license": "ISC",
  "author": "Miroslav Jancarik <miroslav.jancarik@firma.seznam.cz>",
  "contributors": [
    {
      "name": "Jan Šimeček",
      "email": "jan.simecek@firma.seznam.cz"
    }
  ],
  "main": "dist/main.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development webpack --watch",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "publish-webstore": "npm run build && npm run upload",
    "stylelint": "stylelint './**/*.less'",
    "test": "jest --no-watchman -c jest.config.js",
    "upload": "webstore upload --source ./dist --auto-publish",
    "preversion": "npm test",
    "postversion": "VERSION=\"$(npx -c 'echo \"$npm_package_version\"')\" && git add package.json package-lock.json && git commit -m 'chore(release-devtools): publish' && git tag -a -m 'devtools-v$VERSION' devtools-v$VERSION && git push --follow-tags",
    "version:major": "npm --no-git-tag-version version major",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:patch": "npm --no-git-tag-version version patch"
  },
  "dependencies": {
    "@ima/devtools-scripts": "^17.12.2",
    "@reach/menu-button": "0.16.2",
    "@reach/tooltip": "0.16.2",
    "@reduxjs/toolkit": "1.8.1",
    "@uiw/react-textarea-code-editor": "^2.0.1",
    "clsx": "^1.1.1",
    "easy-uid": "2.0.2",
    "lodash.debounce": "4.0.8",
    "nanoreset": "5.0.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-json-view": "1.21.3",
    "react-redux": "7.2.8",
    "react-tabs": "4.2.0",
    "redux": "4.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/plugin-external-helpers": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "autoprefixer": "10.4.4",
    "babel-loader": "8.2.4",
    "chrome-webstore-upload-cli": "1.2.1",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "cssnano": "5.1.7",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "file-loader": "6.2.0",
    "generate-file-webpack-plugin": "^1.0.1",
    "identity-obj-proxy": "3.0.0",
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "mini-css-extract-plugin": "2.6.0",
    "postcss": "^8.4.12",
    "postcss-loader": "6.2.1",
    "terser-webpack-plugin": "5.3.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
